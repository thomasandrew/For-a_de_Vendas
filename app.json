{
  "expo": {
    "name": "WSGE Forca de Vendas",
    "slug": "ForcaVendas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/icon_1024.png",
    "android" : {
      "icon": "./assets/icon/icon_512.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/android/icon_fg.png",
        "backgroundImage": "./assets/icon/android/icon_bg.png",
        "backgroundColor": "#0D2027"
      }
    },
    "splash": {
      "image": "./assets/splash.png",
